PORT=4000
LOG_LEVEL=info
CORS_ORIGIN=http://localhost:3000,http://localhost:8081
MONGODB_URI=mongodb://localhost:27017/medsentinel
FORECASTING_SERVICE_URL=http://localhost:8000

# ETL Configuration
ENABLE_ETL_SCHEDULER=true
DISEASE_API_URL=https://disease.sh/v3/covid-19
DISEASE_TYPE=COVID-19
TARGET_REGIONS=Pune,Mumbai,Delhi,Bangalore,Chennai,Nagpur,Hyderabad,Kolkata

# Weather API Configuration
# OpenMeteo is free and used by default (no API key needed)
USE_OPENMETEO=true
# Optional: OpenWeatherMap API key (fallback if OpenMeteo fails)
OPENWEATHER_API_KEY=your_openweather_api_key_here
WEATHER_API_URL=https://api.openweathermap.org/data/2.5

# Indian Health Data ETL (optional)
ENABLE_INDIAN_HEALTH_ETL=false
NEWS_API_KEY=your_news_api_key_here

# Forecast Scheduler
ENABLE_FORECAST_SCHEDULER=true

